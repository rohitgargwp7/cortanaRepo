                        <Button.Content>
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center" Height="45">
                                <TextBlock x:Name="refreshStatusText" FontSize="22" Foreground="#4a4a44" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,-3,0,0"/>
                                <Image Source="/View/images/refresh_bar_arrow.png" MaxWidth="22" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="8,0,0,0"/>
                            </StackPanel>
                        </Button.Content>
                    </Button>
                    <phone:LongListSelector x:Name="statusLLS" Grid.Row="1" ItemRealized="statusLLS_ItemRealized" toolkit:TiltEffect.SuppressTilt="True" Background="#DDDDDD" Style="{StaticResource LongListSelectorWithOverlappingScrollBar}">
                        <phone:LongListSelector.ItemTemplate>
                            <DataTemplate>
                                <templateSelector:StatusTemplateSelector Content="{Binding}" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch" VerticalAlignment="Bottom" VerticalContentAlignment="Bottom">
                                    <templateSelector:StatusTemplateSelector.DTFriendRequest>
                                        <DataTemplate>
                                            <Border Tap="statusItem_Tap" Style="{StaticResource StatusUpdateContainerStyle}">
                                                <Grid Margin="20" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="Auto"/>
                                                        <ColumnDefinition Width="*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <Image Source="{Binding UserImage}" MaxHeight="69" MaxWidth="69" VerticalAlignment="Top" HorizontalAlignment="Left" Tap="statusBubblePhoto_Tap"/>
                                                    <Grid HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Column="1" Margin="12,0,0,0">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                        </Grid.RowDefinitions>
                                                        <TextBlock Text="{Binding UserName}" Grid.Row="0" Style="{StaticResource StatusUpdateNameTextBlock}"/>
                                                        <TextBlock TextWrapping="Wrap" Style="{StaticResource StatusUpdateContentTextBlock}" Text="{Binding Path= LocalizedResources.Profile_DoTheSame, Source={StaticResource LocalizedStrings}}" Grid.Row="1"/>
