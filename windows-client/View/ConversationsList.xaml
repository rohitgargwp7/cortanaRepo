                                                            <Button Content="{Binding Path= LocalizedResources.Conversations_Pending_Yes_Btn, Source={StaticResource LocalizedStrings}}" Click="yes_Click" Style="{StaticResource YesButtonStyle}" toolkit:TiltEffect.IsTiltEnabled="True"/>
                                                            <Button Margin="12,0,0,0" Content="{Binding Path= LocalizedResources.Conversations_Pending_NotNow_Btn, Source={StaticResource LocalizedStrings}}" Click="no_Click" Style="{StaticResource NoButtonStyle}" toolkit:TiltEffect.IsTiltEnabled="True"/>
                                                        </StackPanel>
                                                    </Grid>
                                                </Grid>
                                            </Border>
                                        </DataTemplate>
                                    </templateSelector:StatusTemplateSelector.DTFriendRequest>
                                    <templateSelector:StatusTemplateSelector.DTImageStatus>
                                        <DataTemplate>
                                            <Border Tap="statusItem_Tap" Style="{StaticResource StatusUpdateContainerStyle}">
                                                <Grid Margin="20" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                                                    <Grid.RowDefinitions>
