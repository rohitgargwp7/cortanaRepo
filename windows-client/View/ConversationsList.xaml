                                        <DataTemplate>
                                            <Grid Margin="12" HorizontalAlignment="Stretch" Tap="statusItem_Tap" Background="Transparent">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="Auto"/>
                                                    <ColumnDefinition Width="*"/>
                                                </Grid.ColumnDefinitions>
                                                <Image Source="{Binding UserImage}" MaxHeight="69" MaxWidth="69" VerticalAlignment="Top" HorizontalAlignment="Left" Tap="statusBubblePhoto_Tap"/>
                                                <Grid HorizontalAlignment="Left" Grid.Column="1" Margin="20,0,0,0">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                    </Grid.RowDefinitions>
                                                    <TextBlock Text="{Binding UserName}" FontSize="30" Grid.Row="0" FontFamily="Segoe WP Semibold"/>
                                                    <TextBlock TextWrapping="Wrap" FontSize="20" Grid.Row="1" Text="{Binding Path= LocalizedResources.Conversations_AddedYouToFav_Txt_WP8FrndStatus, Source={StaticResource LocalizedStrings}}"/>
                                                    <TextBlock TextWrapping="Wrap" FontSize="18" Margin="0,5,0,0" Foreground="#797979" Text="{Binding Path= LocalizedResources.Profile_DoTheSame, Source={StaticResource LocalizedStrings}}" Grid.Row="2"/>
                                                    <StackPanel Orientation="Horizontal" Grid.Row="3" Margin="-12,0,0,0">
                                                        <Button Background="#2e91d3" Height="65" Width="140" FontSize="20" BorderThickness="0" Content="{Binding Path= LocalizedResources.Conversations_Pending_Yes_Btn, Source={StaticResource LocalizedStrings}}" Foreground="White" Click="yes_Click"/>
                                                        <Button Background="#e9ecee" Height="65" Width="140" FontSize="20" BorderThickness="0" Content="{Binding Path= LocalizedResources.Conversations_Pending_NotNow_Btn, Source={StaticResource LocalizedStrings}}" Foreground="#595959" Click="no_Click"/>
                                                    </StackPanel>
                                                </Grid>
                                            </Grid>
                                        </DataTemplate>
                                    </templateSelector:StatusTemplateSelector.DTFriendRequest>
                                    <templateSelector:StatusTemplateSelector.DTImageStatus>
                                        <DataTemplate>
                                            <Grid HorizontalAlignment="Stretch" Margin="12" Tap="statusItem_Tap" Background="Transparent">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto"/>
                                                    <RowDefinition Height="Auto"/>
                                                    <RowDefinition Height="Auto"/>
                                                </Grid.RowDefinitions>
