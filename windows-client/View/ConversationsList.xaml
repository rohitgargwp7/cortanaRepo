                        <Button.Content>
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center" Height="45">
                                <TextBlock x:Name="refreshStatusText" FontSize="22" Foreground="#4a4a44" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,-3,0,0"/>
                                <Image Source="/View/images/refresh_bar_arrow.png" MaxWidth="22" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="8,0,0,0"/>
                            </StackPanel>
                        </Button.Content>
                    </Button>
                    <phone:LongListSelector x:Name="statusLLS" Grid.Row="1" ItemRealized="statusLLS_ItemRealized">
